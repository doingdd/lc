#!/usr/bin/python
def quick_sort(l):
    length = len(l)  
    if length < 2:
        return l
    quick_s(l,0,length-1)

    return l

def quick_s(l,start,end):
    if start >= end:
        return
    p = partition(l,start,end)
    quick_s(l,start,p-1)
    quick_s(l,p+1,end)
    
def partition(l,start,end):
    pivot = l[end]
    i = start
    for j in range(start,end):
        if l[j] < pivot:
            l[i],l[j] = l[j],l[i]
            i += 1

    l[end],l[i] = l[i],l[end]
    return i

case = [[1,2,3,4,5],[3,2,1],[1,1,3,2,2,1],[5,3],
        [],
        [1],[1,1],[2,2,2],[9,8,7,6,5,4,1,2,3]]

for i in case:
  print "case is ",i
  print "result is ",quick_sort(i)
