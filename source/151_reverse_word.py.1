def reverse_word(s):
    stack = []
    word = ""
    for i in s:
        if i==' ':
            if word:
                stack.append(word)
                word = ""
        else:
            word += i

    if word:
        stack.append(word)

    print stack
    return ' '.join(stack[::-1])


